<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="700.0" minWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SelectController">
   <center>
      <ScrollPane fx:id="closeButton" fitToWidth="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="620.0" prefWidth="1300.0">
          <content>
              <VBox fx:id="vbox" spacing="10.0" />
          </content>
      </ScrollPane>
   </center>
   <bottom>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#closeWindow" text="Close" />
        </items>
      </ToolBar>
   </bottom>
   <top>
      <Label alignment="CENTER" prefHeight="40.0" prefWidth="653.0" text="OUTPUT OF SELECT" textFill="RED" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </top>
</BorderPane>
